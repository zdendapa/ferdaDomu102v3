<?xml version='1.0' encoding='utf-8'?>
<widget id="org.apache.cordova.myApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Ferda domů</name>
    <description>Objednávka piva Ferdinand až do domu</description>
    <author email="pejril@graphico.cz" href="http://www.graphico.cz">Tomáš Pejřil</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="res/fdicon.png" />
    <icon density="hdpi" platform="android" src="res/icon/android/drawable-hdpi/appicon.png" />
    <icon density="ldpi" platform="android" src="res/icon/android/drawable-ldpi/appicon.png" />
    <icon density="mdpi" platform="android" src="res/icon/android/drawable-mdpi/appicon.png" />
    <icon density="xhdpi" platform="android" src="res/icon/android/drawable-xhdpi/appicon.png" />
    <icon density="xxhdpi" platform="android" src="res/icon/android/drawable-xxhdpi/appicon.png" />
    <icon height="60" platform="ios" src="res/icon/ios/appicon-60.png" width="60" />
    <icon height="57" platform="ios" src="res/icon/ios/appicon.png" width="57" />
    <icon height="114" platform="ios" src="res/icon/ios/appicon@2x.png" width="114" />
    <icon height="120" platform="ios" src="res/icon/ios/appicon-60@2x.png" width="120" />
    <icon height="72" platform="ios" src="res/icon/ios/appicon-72.png" width="72" />
    <icon height="144" platform="ios" src="res/icon/ios/appicon-72@2x.png" width="144" />
    <icon height="76" platform="ios" src="res/icon/ios/appicon-76.png" width="76" />
    <icon height="152" platform="ios" src="res/icon/ios/appicon-76@2x.png" width="152" />
    <icon height="29" platform="ios" src="res/icon/ios/appicon-Small.png" width="29" />
    <icon height="58" platform="ios" src="res/icon/ios/appicon-Small@2x.png" width="58" />
    <icon height="40" platform="ios" src="res/icon/ios/appicon-Small-40.png" width="40" />
    <icon height="80" platform="ios" src="res/icon/ios/appicon-Small-40@2x.png" width="80" />
    <icon height="50" platform="ios" src="res/icon/ios/appicon-Small-50.png" width="50" />
    <icon height="100" platform="ios" src="res/icon/ios/appicon-Small-50@2x.png" width="100" />
    <splash height="480" platform="ios" src="res/screen/ios/Default.png" width="320" />
    <splash height="960" platform="ios" src="res/screen/ios/Default@2x.png" width="640" />
    <splash height="1136" platform="ios" src="res/screen/ios/Default-568h@2x.png" width="640" />
    <splash height="768" platform="ios" src="res/screen/ios/Default-Landscape.png" width="1024" />
    <splash height="1536" platform="ios" src="res/screen/ios/Default-Landscape@2x.png" width="2048" />
    <splash height="1024" platform="ios" src="res/screen/ios/Default-Portrait.png" width="768" />
    <splash height="2048" platform="ios" src="res/screen/ios/Default-Portrait@2x.png" width="1536" />
    <splash density="hdpi" platform="android" src="res/screen/android/res-notlong-port-hdpi/default.png" />
    <splash density="ldpi" platform="android" src="res/screen/android/res-notlong-port-ldpi/default.png" />
    <splash density="ldpi" platform="android" src="res/screen-ldpi-portrait.png" />
    <splash density="ldpi" platform="android" src="res/screen-ldpi-landscape.png" />
    <splash density="mdpi" platform="android" src="res/screen/android/res-notlong-port-mdpi/default.png" />
    <splash density="xhdpi" platform="android" src="res/screen/android/res-notlong-port-xhdpi/default.png" />
    <splash density="xxhdpi" platform="android" src="res/screen/android/res-notlong-port-xxhdpi/default.png" />
    <splash density="xxxhdpi" platform="android" src="res/screen/android/res-notlong-port-xxxhdpi/default.png" />
    <engine name="android" spec="~6.3.0" />
    <engine name="ios" spec="~4.5.4" />
    <plugin name="cordova-plugin-app-event" spec="^1.2.1" />
    <plugin name="cordova-plugin-device" spec="^1.1.7" />
    <plugin name="cordova-plugin-local-notification" spec="git+https://github.com/katzer/cordova-plugin-local-notifications.git" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.1.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
